{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "no-undef": 0,
        "max-len": 0
    }
}
